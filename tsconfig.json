{
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
        "moduleResolution": "node",
        "target": "esnext",
     
    
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "types",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
